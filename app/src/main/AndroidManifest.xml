<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codingblocks.eventerest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoapp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">

        <activity
            android:name="com.codingblocks.ChatBot_And_InterestRanker.Initial.SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.codingblocks.ChatBot_And_InterestRanker.Activity.EditExpenditure"></activity>
        <activity android:name="com.codingblocks.ChatBot_And_InterestRanker.Activity.ExpenditureManagement"></activity>
        <activity android:name="com.codingblocks.ChatBot_And_InterestRanker.Activity.StudentBatches"></activity>
        <activity android:name="com.codingblocks.ChatBot_And_InterestRanker.Activity.StudentNamesList"></activity>
        <activity android:name="com.codingblocks.ChatBot_And_InterestRanker.Activity.StudentsInterestList"></activity>

    </application>

</manifest>